{
	"type": "module",
	"name": "rwsdk-guestbook",
	"version": "1.0.0",
	"description": "Guestbook app using rwsdk with alchemy",
	"main": "index.js",
	"keywords": [],
	"author": "oscar gabriel",
	"license": "MIT",
	"private": true,
	"scripts": {
		"dev": "bun alchemy dev --env-file .env.dev",
		"dev:vite": "bun --env-file=.env.dev vite dev",
		"dev:init": "bun db:touch && bun db:generate && bun db:migrate",
		"dev:clean": "bun clean:vite && bun dev",
		"build": "bun clean:vite && vite build",
		"clean:vite": "rm -rf ./node_modules/.vite",
		"clean:all": "rm -rf node_modules && rm -rf bun.lock",
		"typecheck": "tsc --noEmit",
		"check": "biome check --write",
		"check:unsafe": "biome check --unsafe",
		"infra:up": "bun alchemy deploy --env-file .env.prod",
		"infra:down": "bun alchemy destroy --env-file .env.prod",
		"db:touch": "wrangler d1 execute DB --local --command='SELECT 1'",
		"db:generate": "drizzle-kit generate",
		"db:migrate": "drizzle-kit migrate",
		"db:migrate:prod": "wrangler d1 migrations apply DB --remote",
		"db:studio": "drizzle-kit studio --config drizzle.config.ts",
		"db:studio:prod": "bun --env-file=.env.prod drizzle-kit studio"
	},
	"dependencies": {
		"@tailwindcss/vite": "^4.1.13",
		"@tanstack/react-form": "^1.19.3",
		"better-auth": "1.3.7",
		"class-variance-authority": "^0.7.1",
		"clsx": "^2.1.1",
		"drizzle-orm": "^0.44.5",
		"input-otp": "^1.4.2",
		"lucide-react": "^0.542.0",
		"radix-ui": "^1.4.3",
		"react": "^19.1.1",
		"react-dom": "^19.1.1",
		"react-server-dom-webpack": "^19.1.1",
		"rwsdk": "0.3.1",
		"sonner": "^2.0.7",
		"tailwind-merge": "^3.3.1",
		"tailwindcss": "^4.1.13",
		"ua-parser-js": "^2.0.4",
		"zod": "^4.1.5"
	},
	"devDependencies": {
		"@biomejs/biome": "2.2.2",
		"@cloudflare/workers-types": "^4.20250904.0",
		"@types/node": "^24.3.1",
		"@types/react": "^19.1.12",
		"@types/react-dom": "^19.1.9",
		"@vitejs/plugin-react": "^5.0.2",
		"alchemy": "0.64.0",
		"babel-plugin-react-compiler": "^19.1.0-rc.3",
		"drizzle-kit": "^0.31.4",
		"miniflare": "^4.20250902.0",
		"tw-animate-css": "^1.3.8",
		"typescript": "^5.9.2",
		"vite": "6.3.5",
		"wrangler": "^4.34.0"
	},
	"packageManager": "bun@1.2.19",
	"trustedDependencies": [
		"@swc/core",
		"@tailwindcss/oxide",
		"esbuild",
		"sharp",
		"workerd"
	]
}
